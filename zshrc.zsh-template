# zshrc
# source: https://github.com/blessxjalil/dotfiles

export ZSH=$HOME/.dotfiles/oh-my-zsh
ZSH_CUSTOM=$HOME/.dotfiles/custom-oh-my-zsh

# theme
ZSH_THEME="fishy2"

# plugins
plugins=(git)

source $ZSH/oh-my-zsh.sh

# alias
alias zshconfig="vim ~/.zshrc"

# functions
update() {
	sudo apt update -y
	sudo apt dist-upgrade -y
	echo "Updated! üçª"
}
